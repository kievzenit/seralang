package test_package;

func main() int {
    let a = 0;
    let break_count = 2:uint;

    do {
        a += 4;
        if (a == 4) {
            continue;
        }

        while (a < 10) {
            a += 5;

            if (a == 13) {
                while (true) {
                    a += 1;

                    if (a != 20) {
                        continue;
                    }

                    do {
                        a += 2;
                        if (a != 30) {
                            continue;
                        } else {
                            break break_count - 2:uint + 3:uint * 2:uint - 3:uint - 1:uint;
                        }
                    } while (true);

                    break;
                }

                break 2:uint;
                continue;
            }
        }
    } while (true);

    return a;
}
